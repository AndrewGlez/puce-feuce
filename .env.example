# Example environment variables for puce-feuce
# Copy this file to .env and fill in your actual values

# MongoDB connection string
MONGODB_URI=mongodb://<username>:<password>@<host>:<port>/<database>?authSource=admin&tls=false

# Base URL of the API
VITE_API_BASE_URL=http://localhost:3000

# JWT secret key for signing tokens
JWT_SECRET=<your_jwt_secret>

# Default admin user credentials
env.USER_EMAIL=<admin_email@example.com>
USER_EMAIL=<admin_email@example.com>
USER_PASSWORD=<your_admin_password>
